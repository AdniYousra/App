<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <EditText
        android:id="@+id/nbnode"
        android:layout_width="400dp"
        android:layout_height="90dp"
        android:ems="10"
        android:inputType="textPersonName"

        app:layout_constraintBottom_toTopOf="@+id/nbround"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.163"
        tools:ignore="LabelFor,MissingConstraints"
        tools:layout_editor_absoluteX="3dp"
        android:autofillHints="round" />

    <EditText
        android:id="@+id/nbround"
        android:layout_width="387dp"
        android:layout_height="80dp"
        android:layout_marginBottom="56dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="round"
        app:layout_constraintBottom_toTopOf="@+id/possi"
        tools:ignore="HardcodedText,LabelFor,MissingConstraints"
        tools:layout_editor_absoluteX="17dp"
        android:autofillHints="round" />

    <EditText
        android:id="@+id/possi"
        android:layout_width="386dp"
        android:layout_height="72dp"
        android:layout_marginBottom="28dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="possibility"
        app:layout_constraintBottom_toTopOf="@+id/maxi"
        tools:ignore="HardcodedText,LabelFor,MissingConstraints"
        tools:layout_editor_absoluteX="13dp" />

    <EditText
        android:id="@+id/maxi"
        android:layout_width="326dp"
        android:layout_height="70dp"
        android:layout_marginBottom="32dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        app:layout_constraintBottom_toTopOf="@+id/e"
        tools:layout_editor_absoluteX="16dp"
        tools:ignore="HardcodedText,LabelFor,MissingConstraints,UnknownId" />

    <EditText
        android:id="@+id/e"
        android:layout_width="358dp"
        android:layout_height="68dp"
        android:layout_marginBottom="48dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        app:layout_constraintBottom_toTopOf="@+id/button"
        tools:layout_editor_absoluteX="13dp"
        tools:ignore="HardcodedText,LabelFor,MissingConstraints" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Button"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="HardcodedText" />

</androidx.constraintlayout.widget.ConstraintLayout>